{
    "manifest_version": 3,
    "name": "AutoTrader UK Cazoo Hider",
    "version": "1.0",
    "description": "Hides Cazoo and other remote/home delivery car garages",
    "permissions": [
        "webNavigation", "scripting", "activeTab"
    ],
    "host_permissions": [
        "*://*.autotrader.co.uk/car-search*"
    ],
    "background": {
        "scripts": [
            "worker.js"
        ]
    }
}